<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RentalHistory">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView5"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:backgroundTint="@color/darkblue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">


        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/patuaoneregular"
            android:text="Rental History"
            android:textColor="@color/white"
            android:textSize="20dp"
            android:layout_gravity="center">
        </TextView>

    </androidx.cardview.widget.CardView>


    <Spinner
        android:id="@+id/rentalHistorySpinner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView5"
        app:layout_constraintVertical_bias="0.038"
        tools:ignore="TouchTargetSizeCheck"></Spinner>

    <androidx.cardview.widget.CardView
        android:layout_width="350dp"
        android:layout_height="560dp"
        android:layout_gravity="center"
        app:cardCornerRadius="10dp"
        app:cardElevation="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rentalHistorySpinner"
        app:layout_constraintVertical_bias="0.305">



        <androidx.cardview.widget.CardView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:elevation="25dp"
            android:layout_margin="10dp"
            >

            <ImageView
                android:layout_width="match_parent"
                android:layout_gravity="center"
                android:scaleType="centerCrop"
                android:layout_height="match_parent"
                android:src="@drawable/carrent"
                />
        </androidx.cardview.widget.CardView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:layout_margin="10dp"
            android:orientation="vertical">
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:elevation="25dp"
                android:backgroundTint="@color/blue"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                    android:layout_marginLeft="60dp"
                    android:layout_marginTop="-44dp"
                    android:text="SEDAN-SUV"
                    android:textColor="@color/black"
                    android:fontFamily="@font/patuaoneregular"
                    android:layout_gravity="center"
                    android:textAlignment="center"

                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Duration: 3 days"
                    android:layout_marginTop="-18dp"
                    android:layout_gravity="center"
                    android:textColor="@color/black"
                    android:textSize="16dp"
                    android:layout_marginLeft="85dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="7dp"
                    android:textColor="	#ff0000"
                    android:layout_gravity="center"
                    android:text="Total Rental Price: ₱ 6,000"
                    android:textSize="16dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="Started: November 2, 2024"
                    android:textColor="@color/black"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="28dp"
                    android:textSize="14sp"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="Ended: November 5, 2024"
                    android:textColor="@color/black"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="47dp"
                    android:textSize="14sp"
                    />






            </androidx.cardview.widget.CardView>
        </LinearLayout>



        <androidx.cardview.widget.CardView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:elevation="25dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="150dp"

            >

            <ImageView
                android:layout_width="match_parent"
                android:layout_gravity="center"
                android:scaleType="centerCrop"
                android:layout_height="match_parent"
                android:src="@drawable/carrent1"
                />
        </androidx.cardview.widget.CardView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="150dp"
            android:orientation="vertical">
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:elevation="25dp"
                android:backgroundTint="@color/blue"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                    android:layout_marginLeft="60dp"
                    android:layout_marginTop="-44dp"
                    android:text="Toyota Vios XLE CVT"
                    android:textColor="@color/black"
                    android:fontFamily="@font/patuaoneregular"
                    android:layout_gravity="center"
                    android:textAlignment="center"

                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Duration: 2 days"
                    android:layout_marginTop="-18dp"
                    android:layout_gravity="center"
                    android:textColor="@color/black"
                    android:textSize="16dp"
                    android:layout_marginLeft="85dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="7dp"
                    android:textColor="	#ff0000"
                    android:layout_gravity="center"
                    android:text="Total Rental Price: ₱ 8,000"
                    android:textSize="16dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="Started: September 21, 2024"
                    android:textColor="@color/black"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="28dp"
                    android:textSize="14sp"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="Ended: September 23, 2024"
                    android:textColor="@color/black"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="47dp"
                    android:textSize="14sp"
                    />

            </androidx.cardview.widget.CardView>
        </LinearLayout>







        <androidx.cardview.widget.CardView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:elevation="25dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="290dp"

            >

            <ImageView
                android:layout_width="match_parent"
                android:layout_gravity="center"
                android:scaleType="centerCrop"
                android:layout_height="match_parent"
                android:src="@drawable/carrent2"
                />
        </androidx.cardview.widget.CardView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="290dp"
            android:orientation="vertical">
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:elevation="25dp"
                android:backgroundTint="@color/blue"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                    android:layout_marginLeft="60dp"
                    android:layout_marginTop="-44dp"
                    android:text="Honda City"
                    android:textColor="@color/black"
                    android:fontFamily="@font/patuaoneregular"
                    android:layout_gravity="center"
                    android:textAlignment="center"

                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Duration: 5 days"
                    android:layout_marginTop="-18dp"
                    android:layout_gravity="center"
                    android:textColor="@color/black"
                    android:textSize="16dp"
                    android:layout_marginLeft="85dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="7dp"
                    android:textColor="	#ff0000"
                    android:layout_gravity="center"
                    android:text="Total Rental Price: ₱ 15,000"
                    android:textSize="16dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="Started: September 5, 2024"
                    android:textColor="@color/black"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="28dp"
                    android:textSize="14sp"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="Ended: September 10, 2024"
                    android:textColor="@color/black"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="47dp"
                    android:textSize="14sp"
                    />

            </androidx.cardview.widget.CardView>
        </LinearLayout>



    </androidx.cardview.widget.CardView>




</androidx.constraintlayout.widget.ConstraintLayout>