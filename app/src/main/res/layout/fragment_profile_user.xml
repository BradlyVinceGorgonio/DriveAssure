<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".profileUserFragment"
    android:backgroundTint="@color/backgroundlightblue"
>


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="86dp"
        android:layout_height="83dp"
        android:background="@drawable/user_circle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />


    <androidx.cardview.widget.CardView
        android:id="@+id/cardView18"
        android:layout_width="80dp"
        android:layout_height="70dp"
        app:cardCornerRadius="15dp"
        app:cardElevation="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.023">


        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="-10dp"
            android:background="@drawable/car_key_1"
            android:clickable="true"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"></ImageView>

        <TextView
            android:id="@+id/textView13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="3dp"
            android:layout_marginTop="20dp"
            android:text="Car renting"
            android:textColor="@color/black"
            android:textSize="14dp"
            tools:ignore="TextSizeCheck"></TextView>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView16"
        android:layout_width="80dp"
        android:layout_height="70dp"
        app:cardCornerRadius="15dp"
        app:cardElevation="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.024">


        <ImageView
            android:id="@+id/favoritesImage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="-10dp"
            android:background="@drawable/heart"></ImageView>


        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="20dp"
            android:text="Favorites"
            android:textColor="@color/black"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/favoritesImage"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="TextSizeCheck"></TextView>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="80dp"
        android:layout_height="70dp"
        app:cardCornerRadius="15dp"
        app:cardElevation="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.83"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.023">


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="-10dp"
            android:background="@drawable/message_more"></ImageView>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="1dp"
            android:layout_marginTop="20dp"
            android:text="Message"
            android:textColor="@color/black"
            android:textSize="15dp"
            tools:ignore="TextSizeCheck"></TextView>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView4"
        android:layout_width="350dp"
        android:layout_height="440dp"
        app:cardCornerRadius="15dp"
        app:cardElevation="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView16"
        app:layout_constraintVertical_bias="0.074">

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="5dp"
            android:text="Your Account"
            android:textColor="@color/black"
            android:textSize="16dp"
            android:textStyle="bold"></TextView>


        <Button
            android:id="@+id/profileBtn"
            android:layout_width="370dp"
            android:layout_height="43dp"
            android:layout_gravity="center"
            android:layout_marginTop="-130dp"
            android:backgroundTint="@color/white"
            android:drawableLeft="@drawable/user"
            android:drawableRight="@drawable/forwardarrow"
            android:gravity="start|center_vertical"
            android:text="Profile"
            android:textColor="@color/black"
            android:textSize="12dp"
            tools:ignore="TextSizeCheck,TouchTargetSizeCheck" />

        <Button
            android:id="@+id/paymentBtn"
            android:layout_width="370dp"
            android:layout_height="43dp"
            android:layout_gravity="center"
            android:layout_marginTop="-80dp"
            android:backgroundTint="@color/white"
            android:drawableLeft="@drawable/payment_8"
            android:drawableRight="@drawable/forwardarrow"
            android:gravity="start|center_vertical"
            android:text="Payments"
            android:textColor="@color/black"
            android:textSize="12dp"
            tools:ignore="TextSizeCheck,TouchTargetSizeCheck" />


        <Button
            android:id="@+id/rentalHistoryBtn"
            android:layout_width="370dp"
            android:layout_height="43dp"
            android:layout_gravity="center"
            android:layout_marginTop="-30dp"
            android:backgroundTint="@color/white"
            android:drawableLeft="@drawable/history_87"
            android:drawableRight="@drawable/forwardarrow"
            android:gravity="start|center_vertical"
            android:text="Rental History"
            android:textColor="@color/black"
            android:textSize="12dp"
            tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>


        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="-100dp"
            android:layout_marginTop="15dp"
            android:text="Contract Details"
            android:textColor="@color/black"
            android:textSize="16dp"
            android:textStyle="bold"></TextView>

        <Button
            android:id="@+id/userAgreementBtn"
            android:layout_width="370dp"
            android:layout_height="43dp"
            android:layout_gravity="center"
            android:layout_marginTop="100dp"
            android:backgroundTint="@color/white"
            android:drawableLeft="@drawable/agreement_1"
            android:drawableRight="@drawable/forwardarrow"
            android:gravity="start|center_vertical"
            android:text="User Agreement"
            android:textColor="@color/black"
            android:textSize="12dp"
            tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>


        <Button
            android:id="@+id/vehicleDocumentBtn"
            android:layout_width="370dp"
            android:layout_height="43dp"
            android:layout_gravity="center"
            android:layout_marginTop="150dp"
            android:backgroundTint="@color/white"
            android:drawableLeft="@drawable/vehicledocuments"
            android:drawableRight="@drawable/forwardarrow"
            android:gravity="start|center_vertical"
            android:text="Vehicle Documents"
            android:textColor="@color/black"
            android:textSize="12dp"
            tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>


        <Button
            android:id="@+id/contractBtn"
            android:layout_width="370dp"
            android:layout_height="43dp"
            android:layout_gravity="center"
            android:layout_marginTop="52dp"
            android:backgroundTint="@color/white"
            android:drawableLeft="@drawable/contract"
            android:drawableRight="@drawable/forwardarrow"
            android:gravity="start|center_vertical"
            android:text="Contract"
            android:textColor="@color/black"
            android:textSize="12dp"
            tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>


    </androidx.cardview.widget.CardView>

    <ImageButton
        android:id="@+id/logoutBtn"
        android:layout_width="43dp"
        android:layout_height="25dp"
        android:background="@drawable/loguouticon"
        android:layout_marginLeft="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.919"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.01"></ImageButton>

    <TextView
        android:layout_width="54dp"
        android:layout_height="18dp"
        android:text="Sign out"
        android:textColor="@color/red"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logoutBtn"
        app:layout_constraintVertical_bias="0.0"></TextView>



</androidx.constraintlayout.widget.ConstraintLayout>