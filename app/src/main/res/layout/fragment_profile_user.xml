<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".profileUserFragment"
>


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="86dp"
        android:layout_height="83dp"
        android:background="@drawable/user_circle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />

    <ImageView
        android:id="@+id/favoritesImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="564dp"
        android:background="@drawable/heart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.392"></ImageView>

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Car renting"
        android:textColor="@color/black"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TextSizeCheck"></TextView>

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="548dp"
        android:text="Favorites"
        android:textColor="@color/black"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/favoritesImage"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TextSizeCheck"></TextView>

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your Account"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintVertical_bias="0.036"></TextView>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="72dp"
        android:background="@drawable/message_more"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toEndOf="@+id/favoritesImage"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.018"></ImageView>

    <Button
        android:id="@+id/profileBtn"
        android:layout_width="380dp"
        android:layout_height="43dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/user"
        android:drawableRight="@drawable/forwardarrow"
        android:gravity="start|center_vertical"
        android:text="Profile"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:layout_constraintVertical_bias="0.023"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/paymentBtn"
        android:layout_width="380dp"
        android:layout_height="43dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/payment_8"
        android:drawableRight="@drawable/forwardarrow"
        android:gravity="start|center_vertical"
        android:text="Payments"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileBtn"
        app:layout_constraintVertical_bias="0.011"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contract Details"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.057"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notificationBtn"
        app:layout_constraintVertical_bias="0.031"></TextView>

    <Button
        android:id="@+id/userAgreementBtn"
        android:layout_width="380dp"
        android:layout_height="43dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/agreement_1"
        android:drawableRight="@drawable/forwardarrow"
        android:gravity="start|center_vertical"
        android:text="User Agreement"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.051"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>

    <androidx.cardview.widget.CardView

        android:layout_width="250dp"
        android:layout_height="1dp"
        app:layout_constraintBottom_toTopOf="@+id/vehicleDocumentBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rentalHistoryBtn"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/rentalHistoryBtn"
        android:layout_width="380dp"
        android:layout_height="43dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/history_87"
        android:drawableRight="@drawable/forwardarrow"
        android:gravity="start|center_vertical"
        android:text="Rental History"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contractBtn"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>

    <Button
        android:id="@+id/vehicleDocumentBtn"
        android:layout_width="380dp"
        android:layout_height="43dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/vehicledocuments"
        android:drawableRight="@drawable/forwardarrow"
        android:gravity="start|center_vertical"
        android:text="Vehicle Documents"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rentalHistoryBtn"
        app:layout_constraintVertical_bias="0.027"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>

    <Button
        android:id="@+id/logoutBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/black"
        android:text="LOG OUT"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vehicleDocumentBtn"
        app:layout_constraintVertical_bias="0.09" />

    <Button
        android:id="@+id/contractBtn"
        android:layout_width="380dp"
        android:layout_height="43dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/contract"
        android:drawableRight="@drawable/forwardarrow"
        android:gravity="start|center_vertical"
        android:text="Contract"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userAgreementBtn"
        app:layout_constraintVertical_bias="0.008"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>

    <androidx.cardview.widget.CardView
        android:layout_width="250dp"
        android:layout_height="1dp"
        app:layout_constraintBottom_toTopOf="@+id/rentalHistoryBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contractBtn"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView4"
        android:layout_width="250dp"
        android:layout_height="1dp"
        app:layout_constraintBottom_toTopOf="@+id/contractBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userAgreementBtn"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/notificationBtn"
        android:layout_width="380dp"
        android:layout_height="43dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/notification_3"
        android:drawableRight="@drawable/forwardarrow"
        android:gravity="start|center_vertical"
        android:text="Notification"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/paymentBtn"
        app:layout_constraintVertical_bias="0.002"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>

    <androidx.cardview.widget.CardView
        android:id="@+id/some_id"
        android:layout_width="250dp"
        android:layout_height="1dp"
        app:layout_constraintBottom_toTopOf="@+id/paymentBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileBtn"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.cardview.widget.CardView
        android:layout_width="250dp"
        android:layout_height="1dp"
        app:layout_constraintBottom_toTopOf="@+id/notificationBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/paymentBtn"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Message"
        android:textColor="@color/black"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.001"
        tools:ignore="TextSizeCheck"></TextView>


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="560dp"
        android:background="@drawable/car_key_1"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/favoritesImage"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.35"></ImageView>


</androidx.constraintlayout.widget.ConstraintLayout>